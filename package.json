{
  "name": "hardhat-safe-deployer",
  "version": "0.1.0",
  "description": "Hardhat plugin to deploy smart-contract via Safe",
  "homepage": "https://github.com/andrijdavid/hardhat-safe-deployer/",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "yarn rimraf dist && tsc",
    "deploy": "hardhat deploy --network",
    "verify": "hardhat etherscan-verify --force-license --license LGPL-3.0 --network",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrijdavid/hardhat-safe-deployer.git"
  },
  "keywords": [
    "Ethereum",
    "Hardhat",
    "Safe"
  ],
  "author": "david@geek.mg",
  "bugs": {
    "url": "https://github.com/andrijdavid/hardhat-safe-deployer/issues"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.21",
    "hardhat": "^2.14.0",
    "husky": "^5.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "hardhat": "^2.14.0"
  },
  "dependencies": {
    "@types/yargs": "^15.0.10",
    "argv": "^0.0.2",
    "axios": "^0.21.1",
    "dotenv": "^8.0.0",
    "ethers": "^5.7.2",
    "solc": "^0.7.6",
    "yargs": "^16.1.1"
  }
}
